Type: Package
Package: conformalInference.multi
Title: Conformal Inference Tools for Regression with Multivariate
    Response
Version: 1.0.0
Authors@R: c(
    person("Paolo", "Vergottini", , "paolo.vergottini@gmail.com", role = c("aut")),
    person("Matteo", "Fontana", , "matteo.fontana@ec.europa.eu", role = c("aut", "ths","cre")),
